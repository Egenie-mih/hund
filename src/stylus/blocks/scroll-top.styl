.btn-scroll-top
  position fixed
  text-align center
  box-sizing border-box
  padding 42px 0
  bottom 80px
  right 20px
  width 108px
  height 108px
  z-index 5
  text-decoration none
  text-transform uppercase
  color #ffffff
  font-size 18px
  background-color #34d8a6
.btn-scroll-top::after
  content '...'
  display block
  line-height 4px
  margin 0 auto
